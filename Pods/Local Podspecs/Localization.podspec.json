  "script_phases": {
    "name": "Generate Localization",
    "script": "#!/bin/bash\n\nif [[ -f \"${PODS_ROOT}/SwiftGen/bin/swiftgen\" ]]; then\n    cd ${PODS_TARGET_SRCROOT}/Localization/\n    ${PODS_ROOT}/SwiftGen/bin/swiftgen\nelse\n    echo \"warning: SwiftGen is not installed. Run \\'pod install --repo-update\\' to install it.\"\nfi\n",
    "input_files": [
      "Localization/pt-BR.lproj/Localizable.strings"
    ],
    "output_files": [

    ],
    "execution_position": "before_compile"
  },
  "xcconfig": {
    "OTHER_SWIFT_FLAGS": ""
  },
